{
  "name": "trondal-ui",
  "version": "0.0.0-development",
  "author": "Trond Albinussen",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "semantic-release": "semantic-release --branches main"
  },
  "module": "dist/trondal-ui.esm.js",
  "size-limit": [
    {
      "path": "dist/trondal-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/trondal-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^17.0.19",
    "prettier": "2.8.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "semantic-release": "^20.1.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trondal/trondal-ui.git"
  }
}
